{
  "manifest_version": 2,
  "name": "input-phone-number",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js", "input-phone-number.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
